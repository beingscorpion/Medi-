{
  "name": "docent-django-frontend",
  "version": "1.0.0",
  "description": "Frontend assets for Docent Django application",
  "main": "index.js",
  "scripts": {
    "build-css": "tailwindcss -i ./static/css/input.css -o ./static/css/tailwind.css --watch",
    "build-css-prod": "tailwindcss -i ./static/css/input.css -o ./static/css/tailwind.css --minify",
    "dev": "concurrently \"npm run build-css\" \"python manage.py runserver\"",
    "build": "npm run build-css-prod"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "tailwindcss",
    "django",
    "frontend",
    "css"
  ],
  "author": "Your Name",
  "license": "MIT"
}